            android:layout_column="1"
            android:text="Button" />

        <Button
            android:id="@+id/button19"
            android:layout_width="wrap_content"
            android:layout_height="70dp"
            android:layout_row="4"
            android:layout_column="2"
            android:text="Button" />

        <Button
            android:id="@+id/button20"
            android:layout_width="wrap_content"
            android:layout_height="70dp"
            android:layout_row="4"
            android:layout_column="3"
